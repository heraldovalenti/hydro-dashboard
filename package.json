{
  "name": "aes-hydro-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@googlemaps/markerclusterer": "^2.5.1",
    "@mui/icons-material": "^5.16.8",
    "@mui/lab": "^5.0.0-alpha.174",
    "@mui/material": "^6.1.9",
    "@mui/styles": "^5.16.8",
    "@mui/x-date-pickers": "^7.22.3",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "google-maps-react": "^2.0.6",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "http-status-codes": "^1.4.0",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.3.4",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.1",
    "redux": "^4.0.5"
  },
  "scripts": {
    "vite:dev": "vite --port 8443",
    "vite:typecheck": "tsc --noEmit",
    "vite:build": "tsc -b && vite build",
    "vite:lint": "eslint .",
    "start": "env-cmd -f .env.development react-scripts start",
    "start-test": "env-cmd -f .env.test react-scripts start",
    "clean": "rm -rf build/",
    "build": "react-scripts build",
    "build:local": "env-cmd -f .env.local npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:development": "env-cmd -f .env.development npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "build:aes-server": "env-cmd -f conf/.env npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:open": "env-cmd -f .env.test cypress open",
    "deploy-prep:staging": "npm run build:staging",
    "deploy-prep:production": "npm run build:production",
    "deploy:staging": "gsutil cp -r ./build/* gs://hydro-dashboard-staging/",
    "deploy:production": "gsutil cp -r ./build/* gs://hydro-dashboard-production/",
    "deploy-prep": "yarn deploy-prep:staging",
    "deploy": "yarn gcloud-verify hydro-dashboard-283320 && yarn deploy:staging",
    "gcloud-verify": "./gcloud-verify.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^18.3.0",
    "redux-devtools": "^3.5.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1"
  }
}
